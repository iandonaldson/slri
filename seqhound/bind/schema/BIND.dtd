<!ELEMENT BIND-cilflag_general %NULL; >
<!ELEMENT BIND-cilflag_membrane ( BIND-membrane )>
<!ELEMENT BIND-cilflag_inside ( BIND-localize )>
 
 
<!-- Definition of BIND-spec-place-set -->
 
 
<!-- 
 ***************************************************************************
 Specific start and end places for an interaction
 (Human readable)
 
 Field description for BIND-spec-place-set
 *****************************************
 start  = specific location where this interaction takes place
	   (e.g. trans golgi, basal membrane, inner mitochondrial space, etc.)
 end    = specific location where this interaction ends
 ***************************************************************************
 -->
<!ELEMENT BIND-spec-place-set ( 
               BIND-spec-place-set_start ,
               BIND-spec-place-set_end? )>
 
<!ELEMENT BIND-spec-place-set_start ( BIND-spec-place )>
<!ELEMENT BIND-spec-place-set_end ( BIND-spec-place )>
 
 
 
 
<!-- Definition of BIND-spec-place -->
 
 
<!-- 
 ***************************************************************************
 Specific place of an interaction
 (Human readable)
 
 Field description for BIND-spec-place
 *************************************
 descr    = description of this place
 other-db = reference in a cellular location database
 ***************************************************************************
 -->
<!ELEMENT BIND-spec-place ( 
               BIND-spec-place_descr ,
               BIND-spec-place_other-db? )>
 
<!ELEMENT BIND-spec-place_descr ( #PCDATA )>
<!ELEMENT BIND-spec-place_other-db ( BIND-other-db )>
 
 
 
<!-- Definition of BIND-condition-set -->
 
 
<!-- 
 ******************************************
 * Interaction conditions (in BIND-descr) *
 ******************************************
 ***************************************************************************
 A set of experimental conditions.
 
 Field description for BIND-conditions-set
 *****************************************
 max-icid   = the highest icid used in this set
 conditions = set of BIND-condition objects
 ***************************************************************************
 -->
<!ELEMENT BIND-condition-set ( 
               BIND-condition-set_max-icid ,
               BIND-condition-set_conditions )>
 
<!ELEMENT BIND-condition-set_max-icid ( Internal-conditions-id )>
<!ELEMENT BIND-condition-set_conditions ( BIND-condition+ )>
 
 
 
<!-- Definition of BIND-condition -->
 
 
<!-- 
 ***************************************************************************
 An experimental condition that has been used to observe
 this interaction.  Interaction conclusion must be reproducible
 using this information.
 
 Field description for BIND-conditions
 *************************************
 icid        = internal condition id (0..n within interaction record)
 general     = list of possible general experimental conditions
 system      = experimental system used
 exp-form-a  = experimental form of biomolecule A used if different from
               actual biomolecule. (e.g. HIS tagged sequence, ATP analogue)
 exp-form-b  = experimental form of biomolecule B used if different from
               actual biomolecule.
 site        = site(s) on molecule A or B that this experiment detects or
               are involved in this experiment
 descr       = text description (e.g. if 'other' is specified
               in conditions or system)
 other-db    = reference to an experimental method database
 source      = empirical evidence
 genetic-exp = genetic experiment description
 ***************************************************************************
 -->
<!ELEMENT BIND-condition ( 
               BIND-condition_icid ,
               BIND-condition_general ,
               BIND-condition_system ,
               BIND-condition_exp-form-a? ,
               BIND-condition_exp-form-b? ,
               BIND-condition_site? ,
               BIND-condition_descr? ,
               BIND-condition_other-db? ,
               BIND-condition_source? ,
               BIND-condition_genetic-exp? )>
 
<!ELEMENT BIND-condition_icid ( Internal-conditions-id )>
<!ELEMENT BIND-condition_general %ENUM; >
<!ATTLIST BIND-condition_general value ( 
               in-vitro |
               in-vivo |
               in-situ |
               in-silico |
               other )  #REQUIRED >
<!ELEMENT BIND-condition_system ( BIND-experimental-system )>
<!ELEMENT BIND-condition_exp-form-a ( BIND-experimental-form )>
<!ELEMENT BIND-condition_exp-form-b ( BIND-experimental-form )>
<!ELEMENT BIND-condition_site ( BIND-loc-site-ref* )>
<!ELEMENT BIND-condition_descr ( #PCDATA )>
<!ELEMENT BIND-condition_other-db ( BIND-other-db )>
<!ELEMENT BIND-condition_source ( BIND-pub-set )>
<!ELEMENT BIND-condition_genetic-exp ( BIND-genetic-experiment )>
 
 
 
 
 
 
 
 
<!-- Definition of Internal-conditions-id -->
 
 
<!-- 
 ***************************************************************************
 Unique reference to an experimental condition
 
 Field description for BIND-condition-ref
 ****************************************
 from-iid  = interaction that contains the place
 condition = Internal-conditions-id (icid) of the experimental condition
 ***************************************************************************
 -->
<!ELEMENT Internal-conditions-id ( %INTEGER; )>
 
 
 
<!-- Definition of BIND-condition-ref -->
 
<!ELEMENT BIND-condition-ref ( 
               BIND-condition-ref_from-iid ,
               BIND-condition-ref_condition )>
 
<!ELEMENT BIND-condition-ref_from-iid ( Interaction-id )>
<!ELEMENT BIND-condition-ref_condition ( Internal-conditions-id )>
 
 
<!-- Definition of BIND-experimental-system -->
 
 
<!-- 
 ***************************************************************************
 A list of experimental systems
 ***************************************************************************
 -->
<!ELEMENT BIND-experimental-system ( %INTEGER; )>
<!ATTLIST BIND-experimental-system value ( 
               not-specified |
               alanine-scanning |
               affinity-chromatography |
               atomic-force-microscopy |
               autoradiography |
               competition-binding |
               cross-linking |
               deuterium-hydrogen-exchange |
               electron-microscopy |
               electron-spin-resonance |
               elisa |
               equilibrium-dialysis |
               fluorescence-anisotropy |
               footprinting |
               gel-retardation-assays |
               gel-filtration-chromatography |
               hybridization |
               immunoblotting |
               immunoprecipitation |
               immunostaining |
               interaction-adhesion-assay |
               light-scattering |
               mass-spectrometry |
               membrane-filtration |
               monoclonal-antibody-blockade |
               nuclear-translocation-assay |
               phage-display |
               reconstitution |
               resonance-energy-transfer |
               site-directed-mutagenesis |
               sucrose-gradient-sedimentation |
               surface-plasmon-resonance-chip |
               transient-coexpression |
               three-dimensional-structure |
               two-hybrid-test |
               allele-specific-complementation |
               far-western |
               other )  #IMPLIED >
 
 
 
<!-- Definition of BIND-experimental-form -->
 
 
<!-- 
 ***************************************************************************
 Description of the experimental form of a biomolecule used.
 
 Field description for BIND-experimental-form
 ********************************************
 object  = experimental form is a biomolecule 
           (e.g. HIS tagged sequence, ATP analogue)
 profile = experimental form is a profile (in silico experiments)
 gene    = experimental form of a gene (if A or B is a gene)
 ***************************************************************************
 -->
<!ELEMENT BIND-experimental-form ( 
               BIND-experimental-form_object |
               BIND-experimental-form_profile |
               BIND-experimental-form_gene )>
 
<!ELEMENT BIND-experimental-form_object ( BIND-object )>
<!ELEMENT BIND-experimental-form_profile ( BIND-profile )>
<!ELEMENT BIND-experimental-form_gene ( BIND-genotype )>
 
 
 
 
 
<!-- Definition of BIND-genotype -->
 
 
<!-- 
 ***************************************************************************
 Description of the experimental form of a gene.
 
 A collection of all of the alleles of this gene that are present on
 chromosomal and extra-chromosomal genetic elements. Genotype is the
 allelic composition of the gene of interest.
 
 Field description for BIND-genotype
 ***********************************
 tot-copy-num = the total copy number of all the alleles of this gene in
                the organism used in this experiment
 alleles      = the sequence of alleles of this gene in the organism used
                in this experiment
 expression   = the phenotype expression(s) of this collection of alleles
 ***************************************************************************
 -->
<!ELEMENT BIND-genotype ( 
               BIND-genotype_tot-copy-num? ,
               BIND-genotype_alleles ,
               BIND-genotype_expression? )>
 
<!ELEMENT BIND-genotype_tot-copy-num ( BIND-allele-copy-num )>
<!ELEMENT BIND-genotype_alleles ( BIND-allele+ )>
<!ELEMENT BIND-genotype_expression ( BIND-phenotype* )>
 
 
 
 
 
<!-- Definition of BIND-allele-copy-num -->
 
 
<!-- 
 ***************************************************************************
 Description of the copy number of an allele.
 
 Field description for BIND-allele-copy-num
 ******************************************
 high         = high allele copy number
 high-ex      = high, copy number is known
 single       = single allele copy
 wild-type    = wild-type allele copy number
 wild-type-ex = wild-type, copy number is known
 reduced      = reduced allele copy number
 reduced-ex   = reduced, copy number is known
 ***************************************************************************
 -->
<!ELEMENT BIND-allele-copy-num ( 
               BIND-allele-copy-num_high |
               BIND-allele-copy-num_high-ex |
               BIND-allele-copy-num_single |
               BIND-allele-copy-num_wild-type |
               BIND-allele-copy-num_wild-type-ex |
               BIND-allele-copy-num_reduced |
               BIND-allele-copy-num_reduced-ex )>
 
<!ELEMENT BIND-allele-copy-num_high %NULL; >
<!ELEMENT BIND-allele-copy-num_high-ex ( BIND-int-fuzz )>
<!ELEMENT BIND-allele-copy-num_single %NULL; >
<!ELEMENT BIND-allele-copy-num_wild-type %NULL; >
<!ELEMENT BIND-allele-copy-num_wild-type-ex ( BIND-int-fuzz )>
<!ELEMENT BIND-allele-copy-num_reduced %NULL; >
<!ELEMENT BIND-allele-copy-num_reduced-ex ( BIND-int-fuzz )>
 
 
 
 
 
<!-- Definition of BIND-allele -->
 
 
<!-- 
 ***************************************************************************
 Description of an allele - a form of a gene.
 
 Field description for BIND-allele
 *********************************
 genetic-elmt-idx = the index of the genetic element that contains this
                    allele.  E.g. chrom 1A=1, chrom 1B=2, plasmid 1=3...
 id               = a reference to the DNA genome sequence "gene"
 names            = the name(s) of this allele
 form             = what is the experimental form of this allele
 copy-num         = how many copies of this experimental form are present
 is-required      = is this allele required for the genetic interaction.
                    An allele can be present and not functionally involved
 biosource        = the type of genetic element that contains this allele
                    and where it came from. E.g. from a chromosome or from
                    a plasmid from a specific strain.
 descr            = optional text description
 source           = empirical evidence references 
 ***************************************************************************
 -->
<!ELEMENT BIND-allele ( 
               BIND-allele_genetic-elmt-idx ,
               BIND-allele_id ,
               BIND-allele_names? ,
               BIND-allele_form ,
               BIND-allele_copy-num ,
               BIND-allele_is-required? ,
               BIND-allele_biosource ,
               BIND-allele_descr? ,
               BIND-allele_source? )>
 
<!ELEMENT BIND-allele_genetic-elmt-idx ( %INTEGER; )>
<!ELEMENT BIND-allele_id ( BIND-id )>
<!ELEMENT BIND-allele_names ( BIND-allele_names_E* )>
<!ELEMENT BIND-allele_form ( BIND-allele-exp-obj-choice )>
<!ELEMENT BIND-allele_copy-num ( BIND-allele-copy-num )>
<!ELEMENT BIND-allele_is-required %BOOLEAN; >
<!ATTLIST BIND-allele_is-required value ( true | false ) "false" >
<!ELEMENT BIND-allele_biosource ( BioSource )>
<!ELEMENT BIND-allele_descr ( #PCDATA )>
<!ELEMENT BIND-allele_source ( BIND-pub-set )>
 
 
<!ELEMENT BIND-allele_names_E ( #PCDATA )>
 
 
 
 
 
 
<!-- Definition of BIND-allele-exp-obj-choice -->
 
 
<!-- 
 ***************************************************************************
 The choice for experimental form of an allele
 
 Field description for BIND-allele-exp-object-choice
 ***************************************************
 genomic  = the allele is not changed from the sequenced genome
            (referred to by BioSource)
 deletion = the allele has been deleted
 mutation = the allele has been mutated from the genome sequence, explicit
            description of new allele sequence is attached.
 ***************************************************************************
 -->
<!ELEMENT BIND-allele-exp-obj-choice ( 
               BIND-allele-exp-obj-choice_not-specified |
               BIND-allele-exp-obj-choice_genomic |
               BIND-allele-exp-obj-choice_knock-out |
               BIND-allele-exp-obj-choice_mutation )>
 
<!ELEMENT BIND-allele-exp-obj-choice_not-specified %NULL; >
<!ELEMENT BIND-allele-exp-obj-choice_genomic %NULL; >
<!ELEMENT BIND-allele-exp-obj-choice_knock-out %NULL; >
<!ELEMENT BIND-allele-exp-obj-choice_mutation ( BIND-object )>
 
 
 
<!-- Definition of BIND-genetic-experiment -->
 
 
<!-- 
 ***************************************************************************
 Type and some results from a genetic experiment.
 
 Field description for BIND-genetic-experiment
 *********************************************
 result-phenotype = the resulting phenotype
 type             = the type of genetic interaction experiment
 background       = the organism and strain that was used in the experiment
 dep-changes      = this experimental result depends on these changes to the
                    background (e.g. gene disruptions of A and B show a
                    synthetic lethality only when C is knocked out)
 descr            = optional text description
 molecule-present = a molecule that was present during this experiment
                    (e.g. a drug or aptamer)
 environment      = description of the environmental conditions
                    (e.g. temperature)
 ***************************************************************************
 -->
<!ELEMENT BIND-genetic-experiment ( 
               BIND-genetic-experiment_result ,
               BIND-genetic-experiment_type ,
               BIND-genetic-experiment_background ,
               BIND-genetic-experiment_dep-changes? ,
               BIND-genetic-experiment_descr? ,
               BIND-genetic-experiment_molecule-present? ,
               BIND-genetic-experiment_environment? )>
 
<!ELEMENT BIND-genetic-experiment_result ( BIND-genetic-exp-result+ )>
<!ELEMENT BIND-genetic-experiment_type ( BIND-genetic-exp-system )>
<!ELEMENT BIND-genetic-experiment_background ( BIND-genetic-background )>
<!ELEMENT BIND-genetic-experiment_dep-changes ( BIND-allele-change* )>
<!ELEMENT BIND-genetic-experiment_descr ( #PCDATA )>
<!ELEMENT BIND-genetic-experiment_molecule-present ( BIND-object* )>
<!ELEMENT BIND-genetic-experiment_environment ( BIND-param* )>
 
 
 
 
 
 
 
<!-- Definition of BIND-genetic-exp-result -->
 
 
<!-- 
 ***************************************************************************
 Result of a genetic experiment.
 
 Field description for BIND-genetic-exp-result
 *********************************************
 phenotype = the resulting phenotype
 relation  = the relation between A, B and AB in this experiment
 ***************************************************************************
 -->
<!ELEMENT BIND-genetic-exp-result ( 
               BIND-genetic-exp-result_phenotype ,
               BIND-genetic-exp-result_relation )>
 
<!ELEMENT BIND-genetic-exp-result_phenotype ( BIND-phenotype )>
<!ELEMENT BIND-genetic-exp-result_relation ( BIND-genetic-relation )>
 
 
 
 
<!-- Definition of BIND-genetic-relation -->
 
 
<!-- 
 ***************************************************************************
 A genetic relation.  The relation between experimental form of A, B and the
 result of genetic experiment AB.
 
 Field description for BIND-genetic-relation
 *******************************************
 a-wild-type  = TRUE if A is wild-type
 b-wild-type  = TRUE if B is wild-type
 a-eq-b       = TRUE if phenotype of A is the same as that of B
 ab-phenotype = the phenotype of the result of the genetic experiment
 ***************************************************************************
 -->
<!ELEMENT BIND-genetic-relation ( 
               BIND-genetic-relation_a-wild-type ,
               BIND-genetic-relation_b-wild-type ,
               BIND-genetic-relation_a-eq-b ,
               BIND-genetic-relation_ab-phenotype )>
 
<!ELEMENT BIND-genetic-relation_a-wild-type %BOOLEAN; >
<!ATTLIST BIND-genetic-relation_a-wild-type value ( true | false )  #REQUIRED >
<!ELEMENT BIND-genetic-relation_b-wild-type %BOOLEAN; >
<!ATTLIST BIND-genetic-relation_b-wild-type value ( true | false )  #REQUIRED >
<!ELEMENT BIND-genetic-relation_a-eq-b %BOOLEAN; >
<!ATTLIST BIND-genetic-relation_a-eq-b value ( true | false )  #REQUIRED >
<!ELEMENT BIND-genetic-relation_ab-phenotype ( BIND-genetic-ab-phenotype )>
 
 
 
<!-- Definition of BIND-genetic-ab-phenotype -->
 
<!ELEMENT BIND-genetic-ab-phenotype ( 
               BIND-genetic-ab-phenotype_wild-type |
               BIND-genetic-ab-phenotype_a-type |
               BIND-genetic-ab-phenotype_b-type |
               BIND-genetic-ab-phenotype_novel )>
 
<!ELEMENT BIND-genetic-ab-phenotype_wild-type %NULL; >
<!ELEMENT BIND-genetic-ab-phenotype_a-type %NULL; >
<!ELEMENT BIND-genetic-ab-phenotype_b-type %NULL; >
<!ELEMENT BIND-genetic-ab-phenotype_novel ( BIND-synthetic-phenotype )>
 
 
 
<!-- Definition of BIND-synthetic-phenotype -->
 
 
<!-- 
 ***************************************************************************
 Description of a synthetic phenotype
 
 Field description for BIND-synthetic-phenotype
 **********************************************
 modulation-pct-a = modulation percentage of phenotype for A
 modulation-pct-b = modulation percentage of phenotype for B
                    NOTE: 0% of phenotype is wild-type, above 100% is
                    enhanced phenotype (away from wild-type)
 mix              = a mix of A and B phenotypes
 novel            = a completely new phenotype
 ***************************************************************************
 -->
<!ELEMENT BIND-synthetic-phenotype ( 
               BIND-synthetic-phenotype_modulation-pct-a |
               BIND-synthetic-phenotype_modulation-pct-b |
               BIND-synthetic-phenotype_mix |
               BIND-synthetic-phenotype_novel )>
 
<!ELEMENT BIND-synthetic-phenotype_modulation-pct-a ( %INTEGER; )>
<!ELEMENT BIND-synthetic-phenotype_modulation-pct-b ( %INTEGER; )>
<!ELEMENT BIND-synthetic-phenotype_mix ( BIND-mixed-phenotype )>
<!ELEMENT BIND-synthetic-phenotype_novel ( BIND-phenotype )>
 
 
 
 
<!-- Definition of BIND-mixed-phenotype -->
 
 
<!-- 
 ***************************************************************************
 Description of a mixed phenotype
 
 Field description for BIND-mixed-phenotype
 ******************************************
 a-pct = percentage of phenotype for A
 b-pct = percentage of phenotype for B
 ***************************************************************************
 -->
<!ELEMENT BIND-mixed-phenotype ( 
               BIND-mixed-phenotype_a-pct ,
               BIND-mixed-phenotype_b-pct )>
 
<!ELEMENT BIND-mixed-phenotype_a-pct ( %INTEGER; )>
<!ELEMENT BIND-mixed-phenotype_b-pct ( %INTEGER; )>
 
 
<!-- Definition of BIND-genetic-exp-system -->
 
 
<!-- 
 ***************************************************************************
 Enumerated list of possible genetic experiments
 
 These experiments involve a genetic cross between two parents, each
 containing a set of alleles in their genotype.  The offspring have a
 known mixture of alleles from the parents which can confer a phenotype
 change.  Synthetic phenotypes occur e.g. when a double mutant phenotype
 is more than the sum phenotypes of each single mutant.
 
 Field description for BIND-genetic-exp-system
 *********************************************
 synthetic-lethal        = lethal phenotype observed in synthetic experiment
 synthetic-growth-defect = negative phenotype change observed
 synthetic-enhancement   = positive phenotype change observed
 suppression             = a phenotype is suppressed when an allele is added
 epistasis               = mutant gene causing phenotype is acting upstream
                           in the genetic network
 non-complementation     = two mutations fail to complement but are in
                           different genes.
 ***************************************************************************
 -->
<!ELEMENT BIND-genetic-exp-system ( %INTEGER; )>
<!ATTLIST BIND-genetic-exp-system value ( 
               not-specified |
               synthetic-lethal |
               synthetic-growth-defect |
               synthetic-enhancement |
               suppression |
               epistasis |
               non-complementation |
               other )  #IMPLIED >
 
 
 
<!-- Definition of BIND-genetic-background -->
 
 
<!-- 
 ***************************************************************************
 The background of a genetic experiment. E.g. organism and strain info.
 
 Field description for BIND-genetic-background
 *********************************************
 org     = the organism and strain of this background
 ploidy  = the ploidy number of the organism (haploid=1, diploid=2, etc.)
 ploidy-diff = records if certain chromosomes are present in different
               copy numbers than the organism ploidy (e.g. trisomy 21)
 changes = the changes in this background from the genome referenced by
           'org BioSource' in this object (e.g. his-)
           NOTE: these changes are not required for the genetic interaction,
           they merely document changes from a common background.
 ***************************************************************************
 -->
<!ELEMENT BIND-genetic-background ( 
               BIND-genetic-background_org ,
               BIND-genetic-background_ploidy ,
               BIND-genetic-background_ploidy-diff? ,
               BIND-genetic-background_changes )>
 
<!ELEMENT BIND-genetic-background_org ( BioSource )>
<!ELEMENT BIND-genetic-background_ploidy ( BIND-int-fuzz )>
<!ELEMENT BIND-genetic-background_ploidy-diff ( BIND-ploidy-diff )>
<!ELEMENT BIND-genetic-background_changes ( BIND-allele-change+ )>
 
 
 
 
 
 
<!-- Definition of BIND-ploidy-diff -->
 
<!ELEMENT BIND-ploidy-diff ( 
               BIND-ploidy-diff_chromosome ,
               BIND-ploidy-diff_copy-num )>
 
<!ELEMENT BIND-ploidy-diff_chromosome ( %INTEGER; )>
<!ELEMENT BIND-ploidy-diff_copy-num ( BIND-int-fuzz )>
 
 
 
<!-- Definition of BIND-allele-change -->
 
 
<!-- 
 ***************************************************************************
 Description of an allele change from wild-type (the genome form)
 
 Field description for BIND-allele-change
 ****************************************
 old-form = a reference to the allele in the BioSource that is changed
 new-form = a description of the changed allele
 descr    = optional text description
 ***************************************************************************
 -->
<!ELEMENT BIND-allele-change ( 
               BIND-allele-change_old-form ,
               BIND-allele-change_new-form ,
               BIND-allele-change_descr? )>
 
<!ELEMENT BIND-allele-change_old-form ( BIND-allele )>
<!ELEMENT BIND-allele-change_new-form ( BIND-allele )>
<!ELEMENT BIND-allele-change_descr ( #PCDATA )>
 
 
 
 
<!-- Definition of BIND-cons-seq-set -->
 
 
<!-- 
 **********************************************************
 * Interaction conserved sequence comment (in BIND-descr) *
 **********************************************************
 ***************************************************************************
 Conserved sequence comment set
 
 Only relevant for biological sequences.
 (e.g. Derived from multiple alignment information)
 
 Field description for BIND-cons-seq-set
 ***************************************
 a = conserved sequence comment for molecule A
 b = conserved sequence comment for molecule B
 ***************************************************************************
 -->
<!ELEMENT BIND-cons-seq-set ( 
               BIND-cons-seq-set_a? ,
               BIND-cons-seq-set_b? )>
 
<!ELEMENT BIND-cons-seq-set_a ( BIND-conserved-seq )>
<!ELEMENT BIND-cons-seq-set_b ( BIND-conserved-seq )>
 
 
 
 
<!-- Definition of BIND-conserved-seq -->
 
 
<!-- 
 ***************************************************************************
 Conserved sequence comment
 
 Alignment data is not stored here, only the conclusion from it.
 
 Field description for BIND-conserved-seq
 ****************************************
 seq-el   = sequence elements that have been shown to be conserved
 descr    = text description (e.g. method of determining conserved sequence)
 other-db = reference to a conserved sequence database (e.g. BLOCKS)
 source   = empirical evidence
 ***************************************************************************
 -->
<!ELEMENT BIND-conserved-seq ( 
               BIND-conserved-seq_seq-el ,
               BIND-conserved-seq_descr? ,
               BIND-conserved-seq_other-db? ,
               BIND-conserved-seq_source? )>
 
<!ELEMENT BIND-conserved-seq_seq-el ( Seq-loc )>
<!ELEMENT BIND-conserved-seq_descr ( #PCDATA )>
<!ELEMENT BIND-conserved-seq_other-db ( BIND-other-db* )>
<!ELEMENT BIND-conserved-seq_source ( BIND-pub-set )>
 
 
 
 
 
<!-- Definition of BIND-loc -->
 
 
<!-- 
 *******************************************************************
 * Binding location on molecules in an interaction (in BIND-descr) *
 *******************************************************************
 ***************************************************************************
 Binding location on a BIND-object
 
 Field description for BIND-loc
 ******************************
 detailed = atomic level detail of interaction sites
 general  = sequence element level description of interaction sites
 source   = empirical evidence
 ***************************************************************************
 -->
<!ELEMENT BIND-loc ( 
               BIND-loc_detailed? ,
               BIND-loc_general? ,
               BIND-loc_source? )>
 
<!ELEMENT BIND-loc_detailed ( #PCDATA )>
<!ELEMENT BIND-loc_general ( BIND-loc-gen )>
<!ELEMENT BIND-loc_source ( BIND-pub-set )>
 
 
 
 
<!-- Definition of BIND-loc-gen -->
 
 
<!-- 
 ***************************************************************************
 General binding location on a BIND-object
 
 Field description for BIND-loc-gen
 **********************************
 a-sites = list of binding sites on molecule A
 b-sites = list of binding sites on molecule B
 bound   = list of sequence elements from A and B that are bound together
 ***************************************************************************
 -->
<!ELEMENT BIND-loc-gen ( 
               BIND-loc-gen_a-sites? ,
               BIND-loc-gen_b-sites? ,
               BIND-loc-gen_bound? )>
 
<!ELEMENT BIND-loc-gen_a-sites ( BIND-loc-site-set )>
<!ELEMENT BIND-loc-gen_b-sites ( BIND-loc-site-set )>
<!ELEMENT BIND-loc-gen_bound ( BIND-loc-pair* )>
 
 
 
 
 
<!-- Definition of BIND-loc-site-set -->
 
 
<!-- 
 ***************************************************************************
 A set of BIND-loc-site objects
 ***************************************************************************
 -->
<!ELEMENT BIND-loc-site-set ( 
               BIND-loc-site-set_max-slid ,
               BIND-loc-site-set_sites )>
 
<!ELEMENT BIND-loc-site-set_max-slid ( BIND-Seq-loc-id )>
<!ELEMENT BIND-loc-site-set_sites ( BIND-loc-site+ )>
 
 
 
<!-- Definition of BIND-loc-site -->
 
 
<!-- 
 ***************************************************************************
 A graph describing which sites on A bind to which sites on B
 	BIND-loc-site objects are nodes (verteces) in the graph
 	BIND-loc-pair objects are edges in the graph
 
 Field description for BIND-loc-site
 ***********************************
 slid     = internal ID of this sequence element
            (0..n within interaction record)
 site     = a sequence element (point or interval)
 sub-unit = if molecule A or B is a molecular complex, specifies which
            sub-unit the site is on.
 descr    = description of this binding site
 source   = empirical evidence
 ***************************************************************************
 -->
<!ELEMENT BIND-loc-site ( 
               BIND-loc-site_slid ,
               BIND-loc-site_site ,
               BIND-loc-site_sub-unit? ,
               BIND-loc-site_descr? ,
               BIND-loc-site_source? )>
 
<!ELEMENT BIND-loc-site_slid ( BIND-Seq-loc-id )>
<!ELEMENT BIND-loc-site_site ( Seq-loc )>
<!ELEMENT BIND-loc-site_sub-unit ( BIND-complex-subunit )>
<!ELEMENT BIND-loc-site_descr ( #PCDATA )>
<!ELEMENT BIND-loc-site_source ( BIND-pub-set )>
 
 
 
 
 
<!-- Definition of BIND-loc-pair -->
 
 
<!-- 
 ***************************************************************************
 A pair of binding sites that are bound to each other
 
 Field description for BIND-loc-pair
 ***********************************
 a-slid = the Seq-loc pointed to by this ID is connected to...
 b-slid = the Seq-loc pointed to by this ID
 source = empirical evidence
 ***************************************************************************
 -->
<!ELEMENT BIND-loc-pair ( 
               BIND-loc-pair_a-slid ,
               BIND-loc-pair_b-slid ,
               BIND-loc-pair_source? )>
 
<!ELEMENT BIND-loc-pair_a-slid ( BIND-Seq-loc-id )>
<!ELEMENT BIND-loc-pair_b-slid ( BIND-Seq-loc-id )>
<!ELEMENT BIND-loc-pair_source ( BIND-pub-set )>
 
 
 
<!-- Definition of BIND-Seq-loc-id -->
 
 
<!-- 
 ***************************************************************************
 Unique reference to a site on a biomolecule
 
 Field description for BIND-loc-site-ref
 ***************************************
 from-iid = interaction that contains the binding site
 molecule = the molecule in the interaction (from-iid) that this site is on
            (A or B)
 site     = BIND-Seq-loc-id (slid) of the binding site
 ***************************************************************************
 -->
<!ELEMENT BIND-Seq-loc-id ( %INTEGER; )>
 
 
 
<!-- Definition of BIND-loc-site-ref -->
 
<!ELEMENT BIND-loc-site-ref ( 
               BIND-loc-site-ref_from-iid ,
               BIND-loc-site-ref_molecule ,
               BIND-loc-site-ref_site )>
 
<!ELEMENT BIND-loc-site-ref_from-iid ( Interaction-id )>
<!ELEMENT BIND-loc-site-ref_molecule %ENUM; >
<!ATTLIST BIND-loc-site-ref_molecule value ( 
               a |
               b )  #REQUIRED >
<!ELEMENT BIND-loc-site-ref_site ( BIND-Seq-loc-id )>
 
 
<!-- Definition of BIND-action-set -->
 
 
<!-- 
 ***********************************************
 * Interaction chemical action (in BIND-descr) *
 ***********************************************
 ***************************************************************************
 A set of chemical actions
 
 Chemical actions mediated by a molecule (A or B) in the 
 interaction (a set because a kinase may phosphorylate a protein multiple 
 times)
 
 Field description for BIND-action-set
 *************************************
 max-iaid = the highest iaid used in this set
 actions  = set of BIND-action objects
 ***************************************************************************
 -->
<!ELEMENT BIND-action-set ( 
               BIND-action-set_max-iaid ,
               BIND-action-set_actions )>
 
<!ELEMENT BIND-action-set_max-iaid ( Internal-action-id )>
<!ELEMENT BIND-action-set_actions ( BIND-action+ )>
 
 
 
<!-- Definition of BIND-action -->
 
 
<!-- 
 ***************************************************************************
 A chemical action
 
 Field description for BIND-action
 *********************************
 iaid      = internal action id (unique identifier for this action in a set)
             (0..n within interaction record)
 descr     = text description (e.g. if 'other' is specified for type)
 direction = direction of chemical action
 type      = type of chemical action
 result    = the product(s) of this chemical action 
  NOTE    this field holds the exact chemical form that is produced, and is
          used by reference by the next interaction acting on the "product".
          For a biopolymer this holds the atoms&bonds representation of the 
          molecule.
 diff       = the atomic level detail of differences created by this action
 signal     = more general kinetics, signal transduction
 kinetics   = chemical action kinetics
 conditions = link to experimental conditions used to observe this action,
		e.g. if there were multiple experimental conditions stored in 
              this interaction record and this action was only seen using
              some of them.
 sub-unit-a  = if molecule A is a molecular complex, specifies the sub-unit
               to which this chemical action applies
 sub-unit-b  = if molecule B is a molecular complex, specifies the sub-unit
               to which this chemical action applies
 action-site = sites on molecule A and B that are involved in the action
 other-db    = reference to a database of chemical actions
 source      = empirical evidence
 ***************************************************************************
 -->
<!ELEMENT BIND-action ( 
               BIND-action_iaid ,
               BIND-action_descr? ,
               BIND-action_direction ,
               BIND-action_type ,
               BIND-action_result? ,
               BIND-action_diff? ,
               BIND-action_signal? ,
               BIND-action_kinetics? ,
               BIND-action_conditions? ,
               BIND-action_sub-unit-a? ,
               BIND-action_sub-unit-b? ,
               BIND-action_action-sites? ,
               BIND-action_other-db? ,
               BIND-action_source? )>
 
<!ELEMENT BIND-action_iaid ( Internal-action-id )>
<!ELEMENT BIND-action_descr ( #PCDATA )>
<!ELEMENT BIND-action_direction ( BIND-direction )>
<!ELEMENT BIND-action_type ( BIND-action-type )>
<!ELEMENT BIND-action_result ( BIND-result-object* )>
<!ELEMENT BIND-action_diff ( #PCDATA )>
<!ELEMENT BIND-action_signal ( BIND-signal )>
<!ELEMENT BIND-action_kinetics ( BIND-kinetics )>
<!ELEMENT BIND-action_conditions ( BIND-condition-ref* )>
<!ELEMENT BIND-action_sub-unit-a ( BIND-complex-subunit )>
<!ELEMENT BIND-action_sub-unit-b ( BIND-complex-subunit )>
<!ELEMENT BIND-action_action-sites ( BIND-action-site )>
<!ELEMENT BIND-action_other-db ( BIND-other-db )>
<!ELEMENT BIND-action_source ( BIND-pub-set )>
 
 
 
 
 
 
 
 
 
 
 
 
<!-- Definition of Internal-action-id -->
 
<!ELEMENT Internal-action-id ( %INTEGER; )>
 
 
 
<!-- Definition of BIND-result-object -->
 
<!ELEMENT BIND-result-object ( 
               BIND-result-object_irid ,
               BIND-result-object_object )>
 
<!ELEMENT BIND-result-object_irid ( Internal-result-id )>
<!ELEMENT BIND-result-object_object ( BIND-object )>
 
 
 
<!-- Definition of Internal-result-id -->
 
 
<!-- 
 ***************************************************************************
 Unique reference to a chemical action or a chemical action result object
 
 Field description for BIND-action-ref
 *************************************
 from-iid = interaction that contains the chemical action
 action   = internal action ID number of the chemical action
 irid     = reference to chemical result from action
            (0..n within interaction record)
 ***************************************************************************
 -->
<!ELEMENT Internal-result-id ( %INTEGER; )>
 
 
 
<!-- Definition of BIND-action-ref -->
 
<!ELEMENT BIND-action-ref ( 
               BIND-action-ref_from-iid ,
               BIND-action-ref_action ,
               BIND-action-ref_irid? )>
 
<!ELEMENT BIND-action-ref_from-iid ( Interaction-id )>
<!ELEMENT BIND-action-ref_action ( Internal-action-id )>
<!ELEMENT BIND-action-ref_irid ( Internal-result-id )>
 
 
<!-- Definition of BIND-direction -->
 
 
<!-- 
 ***************************************************************************
 A direction
 ***************************************************************************
 -->
<!ELEMENT BIND-direction %ENUM; >
<!ATTLIST BIND-direction value ( 
               none |
               a-to-a |
               a-to-b |
               b-to-b |
               b-to-a |
               other )  #REQUIRED >
 
 
 
<!-- Definition of BIND-action-type -->
 
 
<!-- 
 ***************************************************************************
 The type of action and object of that action
 
 Action type		object of that action
 add			BIND-object or NULL
 remove		BIND-object or NULL
 cut-seq		Seq-loc or NULL
 
 Field description for BIND-action-type
 **************************************
 -not-specified = action is not-specified (unknown)
 -none = no chemical action, but e.g. kinetics information needs to be
	   stored (action is known to be nothing)
 -add = add an object (e.g. phosphate) to an object
 -remove = remove an object (e.g. phosphate) from an object
 -bond-break = non-sequence cut action - e.g. small molecule hydrolysis
 -cut-seq = cut a sequence, location may be specified
         (e.g. restriction enzyme)
 -change-conformation = a change in conformation of a molecule
         (e.g. hck protein -> phosphorylation causes conformational change)
 -change-configuration = a change in configuration of a molecule
         (e.g. by an epimerase or isomerase)
 -change-other = another type of change (e.g. metal ion exchange)
 -other = another action
 
 Field description for BIND-action-object
 ****************************************
 none     = no action object
 object   = any BIND-object that is added or removed (e.g. phosphate)
 location = location where a sequence was cut
 ***************************************************************************
 -->
<!ELEMENT BIND-action-type ( 
               BIND-action-type_not-specified |
               BIND-action-type_none |
               BIND-action-type_add |
               BIND-action-type_remove |
               BIND-action-type_bond-break |
               BIND-action-type_cut-seq |
               BIND-action-type_change-conformation |
               BIND-action-type_change-configuration |
               BIND-action-type_change-other |
               BIND-action-type_other )>
 
<!ELEMENT BIND-action-type_not-specified %NULL; >
<!ELEMENT BIND-action-type_none %NULL; >
<!ELEMENT BIND-action-type_add ( BIND-action-object )>
<!ELEMENT BIND-action-type_remove ( BIND-action-object )>
<!ELEMENT BIND-action-type_bond-break %NULL; >
<!ELEMENT BIND-action-type_cut-seq ( BIND-action-object )>
<!ELEMENT BIND-action-type_change-conformation %NULL; >
<!ELEMENT BIND-action-type_change-configuration %NULL; >
<!ELEMENT BIND-action-type_change-other %NULL; >
<!ELEMENT BIND-action-type_other %NULL; >
 
 
 
 
 
<!-- Definition of BIND-action-object -->
 
<!ELEMENT BIND-action-object ( 
               BIND-action-object_none |
               BIND-action-object_object |
               BIND-action-object_location )>
 
<!ELEMENT BIND-action-object_none %NULL; >
<!ELEMENT BIND-action-object_object ( BIND-object )>
<!ELEMENT BIND-action-object_location ( Seq-loc )>
 
 
 
 
<!-- Definition of BIND-signal -->
 
 
