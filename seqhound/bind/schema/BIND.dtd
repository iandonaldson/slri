<!ELEMENT BIND-Pathway-set_database ( BIND-Database-site )>
<!ELEMENT BIND-Pathway-set_pathways ( BIND-Pathway+ )>
 
 
 
 
 
<!-- Definition of BIND-Pathway -->
 
 
<!-- 
 ***************************************************************************
 A pathway record.
 
 A collection of one or more interactions that form a pathway.
 i.e. Two or more BIND-objects that are generally free from each
 other, but form a network of interactions.
 
 Field description for BIND-pathway
 **********************************
 date       = date of record entry
 updates    = a list of updates for the record
 pid        = pathway accession number
 pathway    = a collection of interactions and signal modification objects
 descr      = description of a pathway
 source     = empirical evidence references
 authors    = person(s) who authored this record
 collection = interaction is part of a record collection
              (i.e. a satellite BIND database)
 priv       = TRUE if this pathway is private
 ***************************************************************************
 -->
<!ELEMENT BIND-Pathway ( 
               BIND-Pathway_date ,
               BIND-Pathway_updates? ,
               BIND-Pathway_pid ,
               BIND-Pathway_pathway ,
               BIND-Pathway_descr ,
               BIND-Pathway_source ,
               BIND-Pathway_authors? ,
               BIND-Pathway_collection? ,
               BIND-Pathway_priv? )>
 
<!ELEMENT BIND-Pathway_date ( Date )>
<!ELEMENT BIND-Pathway_updates ( BIND-update-object* )>
<!ELEMENT BIND-Pathway_pid ( Pathway-id )>
<!ELEMENT BIND-Pathway_pathway ( Interaction-id+ )>
<!ELEMENT BIND-Pathway_descr ( BIND-path-descr )>
<!ELEMENT BIND-Pathway_source ( BIND-pub-set )>
<!ELEMENT BIND-Pathway_authors ( Author* )>
<!ELEMENT BIND-Pathway_collection ( BIND-Rec-coll-descr )>
<!ELEMENT BIND-Pathway_priv %BOOLEAN; >
<!ATTLIST BIND-Pathway_priv value ( true | false ) "false" >
 
 
 
 
 
 
 
 
 
<!-- Definition of Pathway-id -->
 
 
<!-- 
 ***************************************************************************
 Pathway description
 
 Field description for BIND-path-descr
 *************************************
 descr              = text description of pathway
	   (e.g. lipid biosynthesis, bacterial chemotaxis, Ras pathway, etc.)
 cell-cycle         = stage of a cell cycle that this pathway is in effect
 pathological-state = disease manifestation if this pathway is present
 pathway-actions    = list of chemical actions that occur in the pathway.
                      Specified actions can only come from interactions in
                      this pathway.
 phenotype          = the normal phenotype of this pathway
 ***************************************************************************
 -->
<!ELEMENT Pathway-id ( %INTEGER; )>
 
 
 
<!-- Definition of BIND-path-descr -->
 
<!ELEMENT BIND-path-descr ( 
               BIND-path-descr_descr? ,
               BIND-path-descr_cell-cycle? ,
               BIND-path-descr_pathological-state? ,
               BIND-path-descr_pathway-actions? ,
               BIND-path-descr_phenotype? )>
 
<!ELEMENT BIND-path-descr_descr ( #PCDATA )>
<!ELEMENT BIND-path-descr_cell-cycle ( BIND-cellstage* )>
<!ELEMENT BIND-path-descr_pathological-state ( BIND-pathol-state* )>
<!ELEMENT BIND-path-descr_pathway-actions ( BIND-action-ref* )>
<!ELEMENT BIND-path-descr_phenotype ( BIND-phenotype* )>
 
 
 
 
 
 
<!-- Definition of BIND-pathol-state -->
 
 
<!-- 
 ***************************************************************************
 Pathological state
 
 Description of a disease that is caused by a change in an interaction in a
 'physiologically normal' pathway.
 
 Field description for BIND-pathol-state
 ***************************************
 pathway-iid  = interaction in the physiologically normal pathway
 cause        = change to the interaction that cause the pathological state
                Choice of interaction was ablated or replaced by another
                interaction
 pathol-state = names(s) of the pathological state
 database     = database(s) that contain information about this disease
                (e.g. OMIM at http://www.ncbi.nlm.nih.gov/Omim/)
 phenotype    = phenotype of this disease state
 descr        = description of the pathological state
 source       = empirical evidence references
 ***************************************************************************
 -->
<!ELEMENT BIND-pathol-state ( 
               BIND-pathol-state_pathway-iid ,
               BIND-pathol-state_cause ,
               BIND-pathol-state_pathol-state ,
               BIND-pathol-state_database? ,
               BIND-pathol-state_phenotype? ,
               BIND-pathol-state_descr? ,
               BIND-pathol-state_source? )>
 
<!ELEMENT BIND-pathol-state_pathway-iid ( Interaction-id )>
<!ELEMENT BIND-pathol-state_cause ( BIND-pathol-state-cause )>
<!ELEMENT BIND-pathol-state_pathol-state ( BIND-pathol-state_pathol-state_E+ )>
<!ELEMENT BIND-pathol-state_database ( BIND-other-db* )>
<!ELEMENT BIND-pathol-state_phenotype ( BIND-phenotype* )>
<!ELEMENT BIND-pathol-state_descr ( #PCDATA )>
<!ELEMENT BIND-pathol-state_source ( BIND-pub-set )>
 
 
<!ELEMENT BIND-pathol-state_pathol-state_E ( #PCDATA )>
 
 
 
 
 
<!-- Definition of BIND-pathol-state-cause -->
 
<!ELEMENT BIND-pathol-state-cause ( 
               BIND-pathol-state-cause_destroyed |
               BIND-pathol-state-cause_replaced-by )>
 
<!ELEMENT BIND-pathol-state-cause_destroyed %NULL; >
<!ELEMENT BIND-pathol-state-cause_replaced-by ( Interaction-id )>
 
 
<!-- Definition of BIND-phenotype -->
 
 
<!-- 
 ***************************************************************************
 Phenotype
 
 Field description for BIND-phenotype
 ************************************
 trait     = trait (e.g. colour)
 name      = name of phenotype (e.g. red)
 wild-type = TRUE if this phenotype is wild-type with respect to the genome
 descr     = optional text description
 db-links  = links to other databases
             E.g.
             GO - (http://www.geneontology.org) can be referenced in the
                  form GO:0003684 damaged DNA binding
             DGAP - (http://dgap.harvard.edu)
 
             E.g. GO can be referenced using a BIND-other-db object like so:
             BIND-other-db ::= SEQUENCE {
                dbname "GO",
                intp 0003684	(use INTEGER field wherever possible)
             }
 
 source    = empirical evidence references
 ***************************************************************************
 -->
<!ELEMENT BIND-phenotype ( 
               BIND-phenotype_trait? ,
               BIND-phenotype_name ,
               BIND-phenotype_wild-type ,
               BIND-phenotype_descr? ,
               BIND-phenotype_db-links? ,
               BIND-phenotype_source? )>
 
<!ELEMENT BIND-phenotype_trait ( #PCDATA )>
<!ELEMENT BIND-phenotype_name ( #PCDATA )>
<!ELEMENT BIND-phenotype_wild-type %ENUM; >
<!ATTLIST BIND-phenotype_wild-type value ( 
               not-specified |
               true |
               false )  #REQUIRED >
<!ELEMENT BIND-phenotype_descr ( #PCDATA )>
<!ELEMENT BIND-phenotype_db-links ( BIND-other-db* )>
<!ELEMENT BIND-phenotype_source ( BIND-pub-set )>
 
 
 
 
 
 
